{
    "name": "on-pgschemas",
    "description": "Laravel package for managing postgresql schemas",
    "keywords" : ["laravel", "postgres"],
    "license" : "MIT",
    "authors": [
        {
            "name": "Yassine Dibi",
            "email": "dibiyassine@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^5.5 || ^6.* || ^7.* || ^8",
        "illuminate/console": "~5.5 || ^6.* || ^7.* || ^8",
        "illuminate/database": "~5.5 || ^6.* || ^7.* || ^8"
    },
    "autoload": {
        "psr-4": {
            "PGSchemas\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PGSchemas\\SchemasServiceProvider"
            ],
            "aliases": {
                "PGSchema": "PGSchemas\\Facades\\PGSchema"
            }
        }
    },
    "minimum-stability": "stable"
}
